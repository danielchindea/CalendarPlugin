<?xml version="1.0" encoding="UTF-8"?>

<plugin xmlns="http://www.phonegap.com/ns/plugins/1.0"
           id="com.themobile.parseplugin"
      version="0.1.0">
    <name>ParsePlugin</name>
    <description>Parse push plugin for phonegap</description>
    <license>MIT</license>
    <keywords>phonegap,parse,push</keywords>


    <js-module src="www/js/plugin-parse.js" name="ParsePlugin">
        <!--<clobbers target="window.calendar" />-->
    </js-module>

    <!-- android -->
    <platform name="android">
        <config-file target="res/xml/config.xml" parent="/*">
            <feature name="ParsePlugin">
                <param name="android-package" value="com.themobile.hotq/ParsePlugin"/>
            </feature>
        </config-file>

        <source-file src="src/android/ParsePlugin.java" target-dir="src/com/themobile/hotq" />      
     </platform>          
</plugin>
